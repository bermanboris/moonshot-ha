{
  "name": "moonshot-ha",
  "version": "0.0.0-development",
  "description": "Moonshot HA",
  "keywords": [
    "moonshot",
    "ha"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "author": "Boris Berman <boris@berman.solutions>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bermanboris/moonshot-ha.git"
  },
  "resolutions": {
    "tar": ">=4.4.2"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "prelint": "yarn typecheck",
    "lint": "yarn eslint 'src/**/*.{js,ts,tsx}' --fix",
    "prebuild": "rimraf dist",
    "gh-pages": "gh-pages -d docs",
    "build": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "start:prod": "node dist`/index.js",
    "typecheck": "tsc",
    "watch": "nodemon --exec ts-node --files src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "yarn lint && yarn test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "presemantic-release": "yarn build",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged",
    "prepush": "yarn test:prod && yarn build",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^7.6.0",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/debug": "^4.1.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/mongoose": "^5.5.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.0",
    "@types/superagent": "^4.1.1",
    "@types/supertest": "^2.0.7",
    "@types/ua-parser-js": "^0.7.33",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-jest-hoist": "^24.6.0",
    "commitizen": "^3.1.1",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gh-pages": "^2.0.1",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "lint-staged": "^8.1.6",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0",
    "semantic-release": "^15.13.3",
    "semantic-release-cli": "^4.1.2",
    "superagent": "^5.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "inversify": "^5.0.1",
    "mongoose": "^5.5.6",
    "morgan": "^1.9.1",
    "node-fetch": "^2.5.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "ua-parser-js": "^0.7.19"
  }
}
