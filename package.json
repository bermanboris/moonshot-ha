{
  "name": "moonshot-ha",
  "version": "0.0.0-development",
  "description": "Moonshot HA",
  "keywords": [
    "moonshot",
    "ha"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "author": "Boris Berman <boris@berman.solutions>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bermanboris/moonshot-ha.git"
  },
  "resolutions": {
    "tar": ">=4.4.2"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "prelint": "yarn typecheck",
    "lint": "yarn eslint 'src/**/*.{js,ts,tsx}' --fix",
    "prebuild": "rimraf dist",
    "gh-pages": "gh-pages -d docs",
    "build": "tsc",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "start:prod": "node dist`/index.js",
    "typecheck": "tsc",
    "watch": "nodemon --exec ts-node --transpile-only --files src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "yarn lint && yarn test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "presemantic-release": "yarn build",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged",
    "prepush": "yarn test:prod && yarn build",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "docker-compose:down": "docker-compose -f docker-compose.yml down",
    "docker-compose:up": "docker-compose -f docker-compose.yml up -d --build --scale moonshot-ha=2",
    "docker-compose:restart": "yarn docker-compose:down && yarn docker-compose:up"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@shelf/jest-mongodb": "^1.1.0",
    "@types/debug": "^4.1.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-rate-limit": "^3.3.0",
    "@types/ioredis": "^4.0.13",
    "@types/ip": "^1.1.0",
    "@types/jest": "^24.0.15",
    "@types/mongoose": "^5.5.8",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.6.2",
    "@types/superagent": "^4.1.3",
    "@types/supertest": "^2.0.8",
    "@types/ua-parser-js": "^0.7.33",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-jest-hoist": "^24.6.0",
    "commitizen": "^3.1.1",
    "coveralls": "^3.0.5",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "jest-extended": "^0.11.2",
    "lint-staged": "^9.2.0",
    "mongodb-memory-server": "^5.1.5",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "semantic-release": "^17.2.3",
    "semantic-release-cli": "^5.1.1",
    "superagent": "^5.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "ioredis": "^4.11.1",
    "ip": "^1.1.5",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "rate-limit-redis": "^1.6.0",
    "reflect-metadata": "^0.1.13",
    "ua-parser-js": "^0.7.20"
  }
}
